{
  "manifest_version": 2,
  "name": "Google 划词翻译",
  "description": "轻量，快速",
  "version": "3.5.1",
  "icons": {
    "512": "icons/icon.png"
  },
  "permissions": [
    "*://*/*"
  ],
  "browser_action": {
    "default_icon": "icons/icon.png"
  },
  "background": {
    "scripts": [
      "background.js",
      "util.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "css": [
        "popup.css"
      ],
      "all_frames": true
    }
  ]
}